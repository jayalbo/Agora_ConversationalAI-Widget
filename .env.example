# Agora Credentials
# Get these from your Agora Console: https://console.agora.io/
AGORA_APP_ID=your_app_id_here
AGORA_APP_CERTIFICATE=your_app_certificate_here
AGORA_API_KEY=your_api_key_here
AGORA_API_SECRET=your_api_secret_here

# LLM Configuration (for Conversational AI)
# OpenAI API for LLM
LLM_URL=https://api.openai.com/v1/chat/completions
LLM_API_KEY=your_openai_api_key_here
LLM_MODEL=gpt-4o-mini

# TTS Configuration (for voice synthesis)
# Azure Cognitive Services for TTS
TTS_API_KEY=your_azure_tts_api_key_here
TTS_REGION=eastus
